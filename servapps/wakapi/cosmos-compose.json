{
  "cosmos-installer": {
    "form": [
      {
        "name": "WAKAPI_PASSWORD_SALT",
        "label": "Wakapi Pasword Salt",
        "initialValue": "{Password.0}",
        "type": "text"
      }
    ]
  },
  "minVersion": "0.8.0",
  "services": {
    "{ServiceName}": {
      "container_name": "{ServiceName}",
      "image": "ghcr.io/muety/wakapi",
      "environment": ["WAKAPI_PASSWORD_SALT={Context.WAKAPI_PASSWORD_SALT}"],
      "labels": {},
      "devices": [],
      "expose": [],
      "tty": true,
      "stdin_open": true,
      "ports": [],
      "restart": "always",
      "volumes": [
        {
          "Type": "volume",
          "Source": "",
          "Target": "/data"
        }
      ],
      "networks": {},
      "routes": [
        {
          "name": "{ServiceName}",
          "mode": "SERVAPP",
          "description": "Expose {ServiceName} to the internet",
          "useHost": true,
          "target": "http://{ServiceName}:3000",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "StartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
