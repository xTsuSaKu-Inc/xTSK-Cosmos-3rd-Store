{
  "cosmos-installer": {
    "form": [
      {
        "name": "WAKAPI_PASSWORD_SALT",
        "label": "Wakapi Pasword Salt",
        "initialValue": "UFDA0SGnV9Zeb1T6jTvkuWV8FTj7p92d",
        "type": "text"
      }
    ]
  },
  "minVersion": "0.8.0",
  "services": {
    "{ServiceName}": {
      "container_name": "{ServiceName}",
      "image": "ghcr.io/muety/wakapi",
      "environment": ["WAKAPI_PASSWORD_SALT={Context.WAKAPI_PASSWORD_SALT}"],
      "hostname": "{ServiceName}",
      "security_opt": ["seccomp:unconfined"],
      "networks": {},
      "labels": {
        "cosmos-persistent-env": "WAKAPI_PASSWORD_SALT",
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://raw.githubusercontent.com/xTsuSaKu-Inc/xTSK-Cosmos-3rd-Store/main/servapps/wakapi/images/logo.png"
      },
      "volumes": [
        {
          "source": "{ServiceName}-data",
          "target": "/data",
          "type": "volume"
        }
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expost {ServiceName} to the internet",
          "useHost": true,
          "target": "http://{ServiceName}:3000",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "StartShield": {
            "Enabled": true
          }
        }
      ],
      "restart": "unless-stopped"
    },
    "networks": {
      "cosmos-{ServiceName}-default": {}
    }
  }
}
